{
  "name": "rescript-airtable-playground",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "namespace": "false",
  "bs-dependencies": [
    "@rescript/react",
    "@greenlabs/rescript-react-hook-form",
    "@glennsl/rescript-fetch",
    "@greenlabs/rescript-formula-components"
  ],
  "bsc-flags": ["-open Belt"]
}
