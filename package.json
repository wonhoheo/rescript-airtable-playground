{
  "name": "rescript-airtable-playground",
  "version": "1.0.0",
  "scripts": {
    "dev:watch": "concurrently -n rescript,next 'pnpm:res:watch' 'pnpm:dev'",
    "dev": "next dev",
    "build": "pnpm res:build && next build",
    "start": "next start",
    "lint": "concurrently -n rescript,prettier,eslint 'pnpm:lint:rescript' 'pnpm:lint:prettier' 'pnpm:lint:eslint'",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:rescript": "rescript format",
    "res:clean": "rescript clean -with-deps",
    "res:build": "rescript",
    "res:watch": "rescript build -w",
    "prepare": "husky install",
    "help": "rescript format -h"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@greenlabs/formula-components": "0.0.5-next.27",
    "@greenlabs/formula-design-token": "^0.0.4",
    "@greenlabs/rescript-formula-components": "0.0.5-next.27",
    "@greenlabs/rescript-react-hook-form": "^0.11.2",
    "@rescript/react": "^0.11.0",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "rescript": "^10.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@glennsl/rescript-fetch": "^0.2.0",
    "@ryyppy/rescript-promise": "^2.1.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3"
  }
